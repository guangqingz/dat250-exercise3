meta {
  name: create user kristoffer
  type: http
  seq: 1
}

post {
  url: http://localhost:8081/users
  body: json
  auth: inherit
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "username": "kristoffer",
    "email": "kristoffer@example.com"
  }
  
}

vars:pre-request {
  ~username: kristoffer
  ~email: kristoffer@example.com
}

vars:post-response {
  ~tokenvalue: res.body.token
}

assert {
  status: eq 200
}

settings {
  encodeUrl: true
}
